{
  "expo": {
    "name": "ChatX",
    "slug": "chatx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F9E5DD"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.verixence.chatx"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F9E5DD"
      },
      "package": "com.verixence.chatx",
      "versionCode": 3,
      "targetSdkVersion": 35,
      "permissions": [
        "INTERNET",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.verixence.chatx"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "chatx",
    "extra": {
      "eas": {
        "projectId": "74335368-bd65-4cf8-8cda-f64b5a56991d"
      }
    }
  }
}
